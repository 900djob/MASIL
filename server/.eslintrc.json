{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "plugin:prettier/recommended",
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": false,
                "parser": "flow"
            }
        ]
    }
}